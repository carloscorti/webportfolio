{
  "name": "carlosportfolio",
  "version": "1.0.0",
  "private": "true",
  "license": "MIT",
  "engines": {
    "node": "13.12.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "heroku-postbuild": "npm run build-webpack && npm run build-node",
    "start": "NODE_ENV=production pm2-runtime start build/server.js -i max --name appProd",
    "dev": "nodemon --exec babel-node lib/server.js",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start-windows-prod": "set NODE_ENV=production&&pm2 start build/server.js -i max --name appProd",
    "start-prod": "NODE_ENV=production pm2 start build/server.js -i max --name appProd",
    "prod-stop": "pm2 stop all",
    "prod-kill": "pm2 delete all"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "assets-webpack-plugin": "^5.0.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "bootstrap": "4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "css-loader": "^3.6.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "helmet": "^3.23.1",
    "ionicons": "2.0.1",
    "jquery": "^3.5.1",
    "jquery.easing": "1.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.8",
    "pm2": "^4.4.0",
    "popper.js": "^1.16.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "toastr": "^2.1.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "nodemon": "^2.0.4"
  }
}
